<html class="dj_quirks dj_webkit dj_chrome dj_contentbox new_teal">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<title>Signup | LeadPipe - lead ads integrations for Facebook</title>
	<link rel="canonical" href="http://www.pipelead.com/signup">
	<!-- <meta name="apple-itunes-app" content="app-id=366794783"> -->
	<meta name="copyright" content="Copyright (c) 2017 PipeLead. All Rights Reserved.">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=5">
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="apple-mobile-web-app-status-bar-style" content="black">
	<!-- <meta content="//mailchimp.com/browserconfig.xml" name="msapplication-config"> -->
	<meta content="#6232E5" name="msapplication-TileColor">
	<meta content="<%= cloudFrontUrl %>/public/pipelead/images/logo-rombo-100.png" name="msapplication-TileImage">
	<link rel="stylesheet" type="text/css" href="<%= cloudFrontUrl %>/public/pipelead/styles/signup-login/theone.css">
	<link rel="stylesheet" type="text/css" href="<%= cloudFrontUrl %>/public/pipelead/styles/signup-login/pages.css">
	<!-- <link href="https://login.mailchimp.com/images/apple-touch-icon-180x180.png" rel="apple-touch-icon" sizes="180x180"> -->
	<!-- <link href="https://login.mailchimp.com/images/touch-icon-192x192.png" rel="icon" sizes="192x192"> -->
	<link rel="shortcut icon" href="<%= cloudFrontUrl %>/public/pipelead/images/logo-rombo-100.png">
</head>
<body id="login" class="new_teal mcd bg-login !padding-top--lv0 selfclear overflow-auto">
	<div class="line flex">
		<div class="unit padding--lv3 !padding-top-bottom--lv0 padding--lv0@phone flex-col full-width-mobile full-width-legacy-ie size1of3">
			<div class="c-loginContainer flex-auto padding--lv5 !padding-left-right--lv0 !padding-bottom--lv0 margin--lv5 !margin-left-right--lv0 !margin-bottom--lv0 padding--lv0@xltablet margin--lv0@xltablet" id="login-box">
				<div id="content" class="!margin-bottom--lv8">
					<div id="login-form-wrap" class="mc-mini-content centered padding--lv1">
						<div class="alignc margin--lv5 !margin-left-right--lv0 !margin-bottom--lv2">
							<div class="freddie freddie--login centered" style="display: flex;justify-content: center;align-items: center;">
								<a href="/signup" title="PipeLead">
									<img src="<%= cloudFrontUrl %>/public/pipelead/images/logo-rombo-100.png" />
								</a>
							</div>
						</div>
						<div class="lastUnit size1of1 overflow-visible flex-shrink-none">
							<div id="av-flash-block" class="c-flashBlock"> </div>
						</div>
						<div id="login-content" class="line selfclear !margin-bottom--lv6">
							<div class="lastUnit overflow-visible">
								<form action="https://login.mailchimp.com/login/post/" id="signup-form" class="c-largeForm">
									<div class="!margin-bottom--lv5">
										<h1 class="text-align--center !margin-bottom--lv3">Sign up</h1>
										<p class="flex align-items--baseline flex-wrap--wrap"> <span class="padding-right--lv1">Create a free PipeLead account to sync Facebook Lead forms with all your marketing tools. </span>  </p>
									</div>
									<fieldset>
										<div class="line login-field">
											<div class="field-wrapper">
												<label for="username" class="">Email</label>
												<input type="text" name="username" id="email" tabindex="1" <%= email ? null : 'autofocus="autofocus"' %> autocorrect="off" autocapitalize="off" class=" av-text" value="<%= email %>">
											</div>
										</div>
										<div class="line">
											<label for="password" class="float-left ">Password</label>
										</div>
										<div class="line !margin-bottom--lv3">
											<input type="password" name="password" tabindex="2" id="password" maxlength="51" class=" av-password" value="" <%= email ? 'autofocus="autofocus"' : null %>>
										</div>
										<div class="line !margin-bottom--lv5">
											<button type="submit" value="Sign up" class="button-large button-wide full-width p1 !margin-bottom--lv5" tabindex="3">Get stated!</button>
										</div>
										<div class="alignc">
											<p class="inline-block margin--lv1 !margin-top-bottom--lv0 !margin-left--lv0"> Already have an account? <a href="/login"> Log in here </a> </p>
										</div>
									</fieldset>
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="hide-print">
				<div class="line nofloat-tablet full-width-mobile !margin-bottom--lv6 mar-lr-auto--tablet mc-mini-content padding--lv1 centered">
					<p class="lastUnit size1of1 small-meta dim6 full-width"> ©2012–2017 <span>All Rights Reserved. <br/> PipeLead® ‒ InstallThisApp®</span></p>
				</div>
			</div>
		</div>
		<div class="lastUnit flex-col overflow-y !padding-left-right--lv0 hide-mobile hide-legacy-ie full-height-vh size2of3 c-billboard__wrapper">
			<div data-mc-el="loginAnnouncementAnimation" class="full-height" style="display: block; opacity: 1; transform: translateY(0px);">
				<div class="c-billboard v_position--top h_position--left">
					<div class="c-billboard__image" style="background-image: url(<%= cloudFrontUrl %>/public/pipelead/images/billboards/flow.gif)"></div>
					<!-- <div class="c-billboard__cta">
						<h2 class="c-billboard__cta__title margin-bottom--lv4" style="color: #092b31;">Here, there, anywhere: Create campaigns with MailChimp’s mobile app. </h2> <a href="#" class="button c-billboard__cta__button" target="_blank" style="color: #092b31; border-color: #092b31;" role="button" rel="noopener noreferrer">Find Out How</a>
					</div> -->
				</div>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="<%= cloudFrontUrl %>/node_modules/jquery/dist/jquery.min.js"></script>
	<script type="text/javascript" src="<%= cloudFrontUrl %>/node_modules/js-cookie/src/js.cookie.js"></script>
	<script type="text/javascript" src="<%= cloudFrontUrl %>/node_modules/whatwg-fetch/fetch.js"></script>
	<%- include('_login-script') %>
	<%- include('_register-script') %>
	<script>		
		$(document).ready(function(){
			$('#signup-form').on('submit', function(e){
				e.preventDefault()
				var appsOrLeadgen = '<%= redirect %>'
				ITAregister(appsOrLeadgen)
			})
		})
	</script>
	<%- include('../_scripts') %>
	<%- disableSegment  ? include('../_disable-segment') : include('../_segment') %>
	<script type="text/javascript">
		analytics.page('PipeLead Signup')
	</script>
</body>
</html>